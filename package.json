{
  "name": "src",
  "version": "1.0.0",
  "description": "## Purpose of this repository",
  "main": "add.js",
  "directories": {
    "test": "_tests_"
  },
  "type": "module",
  "jest": {
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "html"
    ]
  },
  "bin": {
    "coveralls": "./bin/coveralls.js"
  },
  "scripts": {
    "start": "node src/_tests_/*_test.js",
    "mocha": "_mocha -b -R spec ",
    "test-cov": "nyc npm run mocha",
    "test-coveralls": "nyc npm run mocha && shx cat ./coverage/lcov.info | node ./bin/coveralls.js --verbose",
    "test": "mocha src/_tests_/*_test.js --exit",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "coverage": "nyc --reporter=lcov --reporter=html --reporter=json mocha --require _tests_/*_test.js",
    "test-with-coverage": ""
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "chai": "^4.2.0",
    "coverage": "^0.4.1",
    "lcov-parse": "^1.0.0",
    "mocha": "^8.2.1",
    "npx": "^10.2.2",
    "ny": "^1.7.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aqib-tuni-20/demoApp.git"
  },
  "bugs": {
    "url": "https://github.com/aqib-tuni-20/demoApp/issues"
  },
  "homepage": "https://github.com/aqib-tuni-20/demoApp#readme",
  "devDependencies": {
    "coveralls": "^3.1.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^13.3.0",
    "xo": "^0.35.0"
  },
  "keywords": [],
  "overrides": [
    {
      "files": "test/*.js",
      "envs": [
        "mocha"
      ]
    }
  ]
}
